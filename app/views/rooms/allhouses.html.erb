<div id="boxymoxy" class="container-fluid">
  <div id="what" class="row">
  	<div>
		<h1 id="headermeal">Home Made Meals Near You!</h1>
		</div></div>
		<div id="headerspacer" class="row divide"><hr /></div>
		<% if @rooms %>
			<% @rooms.each do |room|%>
			    <div id="pexel" class="col-md-2 potato">
			    	<%= link_to room_path(room) do %>
			    		<%= image_tag room.photos.first.image %>
			    	<% end %>
			    </div>
			    <div id="mealsummaryall" class="col-md-4"><%= room.listing_name%><br><%= room.address %></div>
			    <div id="mealpriceall" class="col-md-2">$<%= room.price %></div>
			    <div id="avatarall" class="col-md-4">
				<%= link_to user_path(room.user) do %>
					<%= image_tag avatar_url(room.user), class: "img-circle avatar-medium" %>
							<% end %>
			    </div>
			   	<div class="row divide"><hr /></div>
			<% end %>
		<% end %>
	     
	</div>	 
  </div>
</div>

